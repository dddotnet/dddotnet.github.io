BEGIN:VCALENDAR
PRODID:-//Microsoft Corporation//Outlook MIMEDIR//EN
VERSION:1.0
METHOD:PUBLISH
X-PUBLISHED-TTL:PT60M
X-WR-CALNAME:.NET User Group Dresden{% for post in site.posts %}
BEGIN:VEVENT{% assign endDate = post.date | date: '%H' %}
DTSTART:{{ post.date | date: '%Y%m%dT%H%M%S' }}
DTEND:{{ post.date | date: '%Y%m%dT' }}{{ endDate | plus: 2 }}{{post.date | date: '%M%S' }}
DTSTAMP:{{ site.time | date: '%Y%m%dT%H%M%S' }}
LAST-MODIFIED:{{ site.time | date: '%Y%m%dT%H%M%S' }}
TZOFFSETFROM:-0100
TZOFFSETTO:-0100
LOCATION:{{ post.place }}
CATEGORIES:.NET User Group Dresden
CLASS:PUBLIC
DESCRIPTION:{{ post.url }}
SUMMARY:{{ post.title }}
END:VEVENT{% endfor %}
END:VCALENDAR
